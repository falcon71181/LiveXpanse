{
  "name": "livexpanse",
  "version": "1.0.0",
  "description": "twitch livestreaming live streaming chat socket",
  "main": "server.js",
  "scripts": {
    "dev": "npx nodemon",
    "start": "node ./build/server.js",
    "build": "rimraf ./build && npx tsc",
    "vercel-build": "echo building",
    "docker:fetch": "(docker rmi livexpanse-server && npm run docker:build) || npm run docker:build",
    "docker:build": "docker build -t livexpanse-server .",
    "docker:start": "docker start livexpanse-server || docker run -d --name livexpanse-server -p 3333:3333 livexpanse-server",
    "docker:stop": "docker stop livexpanse-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcon71181/LiveXpanse.git"
  },
  "keywords": [
    "twitch",
    "live",
    "streaming",
    "livestreaming",
    "chat",
    "livechat",
    "socket"
  ],
  "author": "falcon71181",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/falcon71181/LiveXpanse/issues"
  },
  "homepage": "https://github.com/falcon71181/LiveXpanse#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/nodemon": "^1.19.6",
    "nodemon": "^3.1.0",
    "typescript": "^5.3.3"
  }
}
